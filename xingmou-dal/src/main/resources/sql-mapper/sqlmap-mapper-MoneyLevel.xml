<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gi.xm.platform.dao.MoneyLevelDAO">
	<resultMap id="BaseResultMap" type="com.gi.xm.platform.pojo.MoneyLevel">
		<id 	column="id" 		 property="id" 			jdbcType="INTEGER" />
		<result column="code" 		 property="code" 		jdbcType="VARCHAR" />
		<result column="title" 		 property="title" 		jdbcType="VARCHAR" />
		<result column="add_time" 	 property="addTime" 	jdbcType="TIMESTAMP" />
		<result column="update_time" property="updateTime" 	jdbcType="TIMESTAMP" />
	</resultMap>

	<delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
		DELETE FROM dw_money_level
		WHERE id = #{id,jdbcType=INTEGER} 
	</delete>
	
	<insert id="insert" parameterType="com.gi.xm.platform.pojo.MoneyLevel" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO dw_money_level (
			code,
			title,
			add_time,
			update_time
		)
		VALUES
		(
			#{code,jdbcType=VARCHAR},
			#{title,jdbcType=VARCHAR},
			#{addTime,jdbcType=TIMESTAMP},
			 UNIX_TIMESTAMP()
		)
	</insert>

	<update id="updateByPrimaryKey" parameterType="com.gi.xm.platform.pojo.MoneyLevel">
		UPDATE dw_money_level
		SET
			<if test="code!=null">code = #{code,jdbcType=VARCHAR},</if>
			<if test="title!=null">title = #{title,jdbcType=VARCHAR},</if>
	    WHERE id = #{id,jdbcType=INTEGER}
	</update>
	
		
		
	<sql id="selectFields">
		id,code,title,add_time,update_time
	</sql>
	
	<select id="selectAll" resultMap="BaseResultMap">
		SELECT
			<include refid="selectFields" />
		FROM
			dw_money_level
	</select>
	
	<select id="queryMoneyLevel" resultMap="BaseResultMap" parameterType="com.gi.xm.platform.query.MoneyLevelQuery">
		SELECT
			<include refid="selectFields" />
		FROM
			dw_money_level
		<include refid="queryWhereAndOrder" />
	</select>
	
	<sql id="queryWhereAndOrder">
		<where>
			<if test="id!=null">AND id = #{id,jdbcType=INTEGER}</if>
			<if test="code!=null and code!=''">AND code = #{code,jdbcType=VARCHAR}</if>
			<if test="title!=null and title!=''">AND title = #{title,jdbcType=VARCHAR}</if>
		</where>
		<choose>
			<when test="order!=null and orderBy!=null and order!='' and orderBy!=''">order by ${orderBy} ${order}</when>
			<otherwise>order by id desc</otherwise>
		</choose>
	</sql>
	
	<select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		SELECT
			<include refid="selectFields" />
		FROM
			dw_money_level
		WHERE
			id = #{id,jdbcType=INTEGER}
	</select>

	<select id="selectByPrimaryKeyForUpdate" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		SELECT
			<include refid="selectFields" />
		FROM
			dw_money_level
		WHERE
			id = #{id,jdbcType=INTEGER}
		FOR UPDATE
	</select>
		
</mapper>