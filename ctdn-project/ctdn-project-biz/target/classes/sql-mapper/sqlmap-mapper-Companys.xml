			<if test="title!=null and title!=''"> title = #{title,jdbcType=VARCHAR},</if>
			<if test="districtId!=null"> district_id = #{districtId,jdbcType=INTEGER},</if>
			<if test="districtName!=null and districtName!=''"> district_name = #{districtName,jdbcType=VARCHAR},</if>
			<if test="districtSubId!=null"> district_sub_id = #{districtSubId,jdbcType=INTEGER},</if>
			<if test="districtSubName!=null and districtSubName!=''"> district_sub_name = #{districtSubName,jdbcType=VARCHAR},</if>
			<if test="employeeNumber!=null"> employee_number = #{employeeNumber,jdbcType=INTEGER},</if>
			<if test="foundDate!=null"> found_date = #{foundDate,jdbcType=DATE},</if>
			<if test="needStatus!=null"> need_status = #{needStatus,jdbcType=INTEGER},</if>
			<if test="createdUid!=null"> created_uid = #{createdUid,jdbcType=BIGINT},</if>
			<if test="createdUname!=null and createdUname!=''"> created_uname = #{createdUname,jdbcType=VARCHAR},</if>
			<if test="updateTime!=null"> update_time = #{updateTime,jdbcType=BIGINT}</if>
		</set>
		<where>
			<trim prefixOverrides="and">
				<if test="id != null"> and id =#{id}</if>
			</trim>
		</where>
	</update>

