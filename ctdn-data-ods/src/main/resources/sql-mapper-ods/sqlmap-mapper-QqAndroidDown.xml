<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gi.ctdn.ods.dao.QqAndroidDownDAO">
	<resultMap id="BaseResultMap" type="com.gi.ctdn.ods.pojo.QqAndroidDown">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="date" property="date" jdbcType="INTEGER" />
		<result column="title" property="title" jdbcType="VARCHAR" />
		<result column="appid" property="appid" jdbcType="BIGINT" />
		<result column="downsum" property="downsum" jdbcType="BIGINT" />

		<result column="downday" property="downday" jdbcType="INTEGER" />
	</resultMap>


	<sql id="selectFields">
		id,date,title,appid,downsum,downday
	</sql>
	
			
	<select id="selectByAppid" resultMap="BaseResultMap">
		SELECT
			<include refid="selectFields" />
		FROM
			qq_android_down
		WHERE
			appid = #{appid,jdbcType=BIGINT}
	</select>
	
</mapper>