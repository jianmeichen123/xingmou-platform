	<select id="selectByYearIndustryId" resultMap="BaseResultMap">
		SELECT
		<include refid="selectFields" />
		FROM dm_chart_project_industry_year
		WHERE  year in
		<foreach item="item" index="index" collection="years" open="(" separator="," close=")">
			#{item}
		</foreach>
		ORDER BY project_num desc
