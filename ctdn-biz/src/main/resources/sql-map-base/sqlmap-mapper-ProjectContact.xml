<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gi.ctdn.dao.ProjectContactDAO">
	<resultMap id="BaseResultMap" type="com.gi.ctdn.pojo.ProjectContact">
                <id column="projectId" property="projectId" jdbcType="INTEGER" />
                <result column="projTitle" property="projTitle" jdbcType="VARCHAR" />
                <result column="city" property="city" jdbcType="VARCHAR" />
                <result column="addr" property="addr" jdbcType="VARCHAR" />
                <result column="zipCode" property="zipCode" jdbcType="VARCHAR" />
                <result column="tel" property="tel" jdbcType="VARCHAR" />
                <result column="mail" property="mail" jdbcType="VARCHAR" />
                <result column="fax" property="fax" jdbcType="VARCHAR" />
                <result column="sourceCode" property="sourceCode" jdbcType="VARCHAR" />
    	</resultMap>


	<sql id="selectFields">
	    projectId,projTitle,city,addr,zipCode,tel,mail,fax,sourceCode
	</sql>
	
			
	<select id="selectBySourceCode" resultMap="BaseResultMap">
		SELECT
            <include refid="selectFields" />
		FROM
		    app_project_contact
		WHERE  sourceCode = #{sourceCode,jdbcType=VARCHAR}
	</select>

	<select id="selectById" resultMap="BaseResultMap">
		SELECT
		<include refid="selectFields" />
		FROM
		app_project_contact
		WHERE  projectId = #{projectId,jdbcType=INTEGER}
	</select>

	<select id="selectAll" resultMap="BaseResultMap">
		SELECT
            <include refid="selectFields" />
		FROM
		    app_project_contact
	</select>

</mapper>