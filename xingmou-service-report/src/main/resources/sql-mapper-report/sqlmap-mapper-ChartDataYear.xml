			invested_per_money,
			invested_median_money,
			invested_max_money,
			add_time,
			update_time
		)
		VALUES
		(
			#{yaar,jdbcType=INTEGER},
			#{marketMoney,jdbcType=BIGINT},
			#{marketMoneyRate,jdbcType=DOUBLE},
			#{marketUser,jdbcType=BIGINT},
			#{marketUserRate,jdbcType=INTEGER},
			#{singleUserValue,jdbcType=BIGINT},
			#{marketMoneyStock,jdbcType=BIGINT},
