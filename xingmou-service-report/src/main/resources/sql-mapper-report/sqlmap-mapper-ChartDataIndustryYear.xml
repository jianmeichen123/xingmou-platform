	<select id="selectByIndustryIdYaar" resultMap="BaseResultMap">
		SELECT
		<include refid="selectFields" />
		FROM dm_chart_data_industry_year
		WHERE  industry_id = #{industryId,jdbcType=INTEGER}
		AND yaar = #{yaar,jdbcType=INTEGER} 
	</select>
			
	<select id="selectByYaarIndustrySubId" resultMap="BaseResultMap">
		SELECT
		<include refid="selectFields" />
		FROM dm_chart_data_industry_year

		WHERE  yaar in
		<foreach item="item" index="index" collection="years" open="(" separator="," close=")">
			#{item}
		</foreach>
		<if test="industrySubId != null and industrySubId > 0">
			AND industry_sub_id = #{industrySubId,jdbcType=INTEGER}
		</if>
	</select>
			
	<select id="selectByIndustryId" resultMap="BaseResultMap">
